{
  "name": "@antongolub/misc",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/topoconfig/*",
    "packages/esbuild/*"
  ],
  "description": "A monorepo for everything",
  "scripts": {
    "toc": "node ./packages/infra/scripts/readme-toc.mjs",
    "build": "yarn bulk:topobuild",
    "test": "yarn bulk:topobuild --no-build --no-publish",
    "cov": "yarn build:esbuild --cwd $PWD/packages/lcov-utils && node ./packages/infra/scripts/cov.mjs"
  },
  "author": "Anton Golub <antongolub@antongolub.com>",
  "license": "MIT",
  "packageManager": "yarn@4.0.2"
}
